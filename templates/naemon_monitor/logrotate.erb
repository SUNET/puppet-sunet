/var/log/naemon/naemon.log {
    daily
    rotate 3650
    nocompress
    olddir archives
    dateext
    dateformat -%Y%m%d
    missingok
    notifempty
    postrotate
      /usr/bin/docker exec naemon_monitor-naemon-1 pkill --signal USR1 -f '/usr/bin/naemon --allow-root /etc/naemon/naemon.cfg'
    endscript
}
