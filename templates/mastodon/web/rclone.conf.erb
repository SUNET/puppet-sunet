[old]
type = s3
provider = Ceph
access_key_id = <%= @aws_access_key_id %>
secret_access_key = <%= @aws_secret_access_key %>
endpoint = <%= @s3_alias_host.split('/')[0] %>
acl = private

[new]
type = s3
provider = Ceph
access_key_id = <%= @aws_access_key_id_new %>
secret_access_key = <%= @aws_secret_access_key_new %>
endpoint = <%= @s3_alias_host_new.split('/')[0] %>
acl = private
