---
py_pdfsigner:
  sign_queue_name: sign
  add_signed_queue_name: add_signed
  pkcs11:
    label: <%= @pkcs11_label %>
    pin:  <%= @pkcs11_pin %>
    module:  <%= @pkcs11_module %>
    key_label:  <%= @pkcs11_key_label %>
    cert_label:  <%= @pkcs11_cert_label %>
    slot: <%= @pkcs11_slot %>
  redis:
    host: <%= @redis_addr %>
    port: <%= @redis_port %>
    db: 3
  metadata:
    location: <%= @meta_location %>
    reason: <%= @meta_reason %>
    name: <%= @meta_name %>
    contact_info: <%= @meta_contact_info %>
    field_name: <%= @meta_field_name %>