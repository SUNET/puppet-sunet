---
common:
  mongo:
    uri: mongodb://<%= @mongo_user %>:<%= @mongo_pw %>@vc_mongo:27017
  production: <%= @production %>
  log:
    level:
    folder_path: "/var/log/sunet"
  tracing:
    addr: <%= @tracing_endpoint_addr %>
    type: jaeger
  key_value:
    db: 3
    addr: vc_redis:6379
    password: <%= @redis_password %>


cache:
  api_server:
    addr: :8080

persistent:
  api_server:
    addr: :8080