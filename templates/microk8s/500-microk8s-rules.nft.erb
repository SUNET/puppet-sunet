table ip filter {
  chain OUTPUT {
    oifname "vxlan.calico" counter packets 0 bytes 0 accept
  }
  chain INPUT {
    iifname "vxlan.calico" counter packets 0 bytes 0 accept
  }
  chain FORWARD {
     type filter hook forward priority filter; policy accept;
  }
}
table ip6 filter {
  chain OUTPUT {
    oifname "vxlan.calico" counter packets 0 bytes 0 accept
  }
  chain INPUT {
    iifname "vxlan.calico" counter packets 0 bytes 0 accept
  }
  chain FORWARD {
     type filter hook forward priority filter; policy accept;
  }
}
