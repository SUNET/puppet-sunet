# Configuration for telegraf agent
# everything else is in /etc/telegraf/telegraf.d

[agent]
  interval = "10s"
  round_interval = true
  metric_batch_size = 1000
  metric_buffer_limit = 10000
  collection_jitter = "0s"
  flush_interval = "10s"
  flush_jitter = "0s"
  precision = ""
  quiet = false
  logfile = ""
  omit_hostname = false

