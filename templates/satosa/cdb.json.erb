{
  "platform": {
    "response_types": ["code", "id_token"],
    "client_id": "<%= @client_id %>",
    "client_secret": "<%= @client_secret %>",
    "claims_supported" : ["id_token"],
    "redirect_uris": [
      "<%= @redirect_uri %>"
    ],
    "allowed_scope_values": [
	    "profile",
	    "email"
    ]
  }
}
